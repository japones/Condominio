<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="SLC" name="br.com.xkinfo.slc.Model.UnidadeConsumidora" table="UNIDADECONSUMIDORA">
        <id name="id" type="java.lang.Integer">
            <column name="ID"/>
            <generator class="identity"/>
        </id>
        <property name="condominio" type="int">
            <column name="CONDOMINIO" not-null="true"/>
        </property>
        <property name="tipouc" type="int">
            <column name="TIPOUC" not-null="true"/>
        </property>
        <property name="ultimonovel" type="boolean">
            <column name="ULTIMONOVEL" not-null="true"/>
        </property>
        <property name="nivelsuperior" type="java.lang.Integer">
            <column name="NIVELSUPERIOR"/>
        </property>
        <property name="numero" type="int">
            <column name="NUMERO" not-null="true"/>
        </property>
        <property name="dataultimaleitura" type="date">
            <column length="10" name="DATAULTIMALEITURA"/>
        </property>
        <property name="usuarioinclusao" type="int">
            <column name="USUARIOINCLUSAO" not-null="true"/>
        </property>
        <property name="datainclusao" type="date">
            <column length="10" name="DATAINCLUSAO" not-null="true"/>
        </property>
        <property name="usuarioalteracao" type="java.lang.Integer">
            <column name="USUARIOALTERACAO"/>
        </property>
        <property name="dataalteracao" type="date">
            <column length="10" name="DATAALTERACAO"/>
        </property>
    </class>
  

</hibernate-mapping>
